{
  "name": "@runno/sandbox",
  "version": "0.10.3",
  "description": "Run code inside the Runno WebAssembly sandbox.",
  "keywords": [
    "wasm",
    "sandbox"
  ],
  "author": "Ben Taylor <me@taybenlor.com>",
  "homepage": "https://runno.dev",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./worker": {
      "types": "./dist/worker.d.ts",
      "import": "./dist/worker.js"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taybenlor/runno.git"
  },
  "scripts": {
    "bootstrap": "cp -R ../../langs/ ./public/langs/",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup && cp -R ./public/langs/ ./dist/langs/"
  },
  "bugs": {
    "url": "https://github.com/taybenlor/runno/issues"
  },
  "dependencies": {
    "@runno/wasi": "^0.10.3",
    "tar": "^7.5.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.54.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
